# Gmail API Credentials
GMAIL_CLIENT_ID=your_client_id_here
GMAIL_CLIENT_SECRET=your_client_secret_here
GMAIL_REDIRECT_URI=http://localhost:8080
GMAIL_CREDENTIALS_PATH=/path/to/credentials.json

# SMTP Configuration (for email-mcp)
SMTP_HOST=smtp.gmail.com
SMTP_PORT=587
SMTP_USER=your_email@gmail.com
SMTP_PASS=your_app_password_here

# Odoo Configuration
ODOO_URL=http://localhost:8069
ODOO_DATABASE=odoo_db
ODOO_API_KEY=your_odoo_api_key_here

# Facebook Configuration (for social-mcp)
FACEBOOK_PAGE_ID=your_facebook_page_id_here
FACEBOOK_ACCESS_TOKEN=your_facebook_access_token_here

# Instagram Configuration (for social-mcp)
INSTAGRAM_USER_ID=your_instagram_business_account_id_here
INSTAGRAM_ACCESS_TOKEN=your_instagram_access_token_here

# LinkedIn Configuration (for social-mcp)
LINKEDIN_CLIENT_ID=your_linkedin_client_id_here
LINKEDIN_CLIENT_SECRET=your_linkedin_client_secret_here
LINKEDIN_ACCESS_TOKEN=your_linkedin_access_token_here
LINKEDIN_ORGANIZATION_ID=your_linkedin_organization_id_here
LINKEDIN_PERSON_URN=urn:li:person:your_linkedin_person_id

# WhatsApp Session (if using Playwright)
WHATSAPP_SESSION_PATH=/path/to/whatsapp/session

# AI Reasoning Engine Selection
# Options: 'claude' (Claude Code - Primary), 'qwen' (Qwen - Secondary)
# Default: 'claude'
AI_REASONING_ENGINE=claude

# Development Mode (set to 'true' to enable dry-run)
DEV_MODE=false

# IMPORTANT: Copy this file to .env and fill in your actual credentials
# NEVER commit .env to version control - it is excluded in .gitignore
# AI_REASONING_ENGINE controls which AI assistant is used for task reasoning
